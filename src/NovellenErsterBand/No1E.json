{
    "workflow": [
        {
            "action": "trim-hidden"
        },
        {
            "action": "trim-empty"
        },
        {
            "action": "merge-styles",
            "params": {
                "extract_style_names": [
                    "margin-left",
                    "margin-right",
                    "margin-top",
                    "margin-bottom",
                    "text-align",
                    "text-indent",
                    "font-name",
                    "font-size",
                    "letter-spacing",
                    "font-style",
                    "font-weight",
                    "text-position"],
                "style_mappings": {
                    "[('font-style', 'normal'), ('font-weight', 'bold')]": "font-weight-bold",
                    "[('font-size', '12pt'), ('letter-spacing', '-0.004cm')]": "font-size-medium",
                    "[('text-position', 'sub 58%')]": "sub",
                    "[('font-size', '12pt'), ('letter-spacing', '0.004cm')]": "font-size-medium",
                    "[('font-size', '12pt'), ('letter-spacing', '-0.0016in')]": "font-size-medium",
                    "[('font-size', '12pt')]": "font-size-medium",
                    "[('font-name', 'Arial'), ('font-size', '9pt')]": "font-arial",
                    "[('font-style', 'normal'), ('font-weight', 'bold'), ('letter-spacing', '-0.004cm')]": "font-weight-bold",
                    "[('font-style', 'normal'), ('font-weight', 'bold'), ('letter-spacing', '-0.007cm')]": "font-weight-bold",
                    "[('font-style', 'normal'), ('font-weight', 'bold'), ('letter-spacing', '-0.0028in')]": "font-weight-bold",
                    "[('font-style', 'normal'), ('font-weight', 'bold'), ('letter-spacing', '-0.0016in')]": "font-weight-bold",
                    "[('font-name', 'Times New Roman'), ('font-size', '10pt'), ('font-style', 'italic'), ('margin-left', '0cm'), ('margin-right', '0cm'), ('text-align', 'justify'), ('text-indent', '0cm')]": "",
                    "[('font-name', 'Times New Roman'), ('font-size', '10pt'), ('font-style', 'italic'), ('margin-left', '0in'), ('margin-right', '0in'), ('text-align', 'justify'), ('text-indent', '0in')]": "",
                    "[('font-name', 'Times New Roman'), ('font-size', '14pt'), ('font-style', 'italic'), ('margin-bottom', '0.0417in'), ('margin-left', '0in'), ('margin-right', '0in'), ('margin-top', '0.1665in'), ('text-align', 'start'), ('text-indent', '0in')]": "",
                    "[('font-name', 'Times New Roman'), ('font-size', '12pt'), ('font-style', 'italic'), ('margin-bottom', '0.0417in'), ('margin-left', '0in'), ('margin-right', '0in'), ('margin-top', '0.1665in'), ('text-align', 'start'), ('text-indent', '0in')]": "",
                    "[('font-name', 'Times New Roman'), ('font-size', '10pt'), ('font-style', 'italic'), ('font-weight', 'bold'), ('margin-left', '0in'), ('margin-right', '0in'), ('text-align', 'justify'), ('text-indent', '0in')]": "",
                    "[('font-style', 'normal'), ('letter-spacing', '-0.007cm')]": "",
                    "[('font-style', 'normal'), ('letter-spacing', '0.053cm')]": "",
                    "[('font-style', 'normal'), ('letter-spacing', '0.004cm')]": "",
                    "[('font-style', 'normal'), ('letter-spacing', '-0.004cm')]": "",
                    "[('font-style', 'normal')]": "",
                    "[('letter-spacing', '-0.004cm')]": "",
                    "[('letter-spacing', '0.004cm')]": "",
                    "[('letter-spacing', '0.053cm')]": "",
                    "[('letter-spacing', '0.035cm')]": "",
                    "[('letter-spacing', '-0.007cm')]": "",
                    "[('letter-spacing', '-0.0016in')]": "",
                    "[('letter-spacing', '-0.0028in')]": "",
                    "[('letter-spacing', '0.0138in')]": "",
                    "[('font-style', 'normal'), ('letter-spacing', '-0.0028in')]": "",
                    "[('font-style', 'normal'), ('letter-spacing', '0.0209in')]": "",
                    "[('font-size', '12pt'), ('letter-spacing', '0.0016in')]": "",
                    "[('letter-spacing', '0.0016in')]": "",
                    "[('font-style', 'normal'), ('letter-spacing', '0.0016in')]": "",
                    "[('font-style', 'normal'), ('letter-spacing', '-0.0016in')]": "",
                    "[('letter-spacing', '0.0209in')]": ""
                }
            }
        },
        {
            "action": "modify-elements",
            "params": {
                "rules": [
                    {
                        "match": {
                            "tag": "{http://www.tei-c.org/ns/1.0}span",
                            "text": "\u00a0"
                        },
                        "action": {
                            "attrs": {
                                "style": null
                            }
                        }
                    },
                    {
                        "match": {
                            "tag": "{http://www.tei-c.org/ns/1.0}span",
                            "text": " "
                        },
                        "action": {
                            "attrs": {
                                "style": null
                            }
                        }
                    },
                    {
                        "match": {
                            "attrs": [
                                {
                                    "name": "style",
                                    "value": "font-arial"
                                }
                            ]
                        },
                        "action": {
                            "tag": "{http://www.tei-c.org/ns/1.0}foreign",
                            "attrs": {
                                "style": null
                            }
                        }
                    }
                ]
            }
        },
        {
            "action": "simplify-tree"
        },
        {
            "action": "modify-elements",
            "params": {
                "rules": [
                    {
                        "match": {
                            "tag": "{http://www.tei-c.org/ns/1.0}body"
                        },
                        "action": {
                            "tag": "{http://www.tei-c.org/ns/1.0}interpGrp",
                            "attrs": {
                                "type": "individual"
                            }
                        }
                    },
                    {
                        "match": {
                            "tag": "{http://www.tei-c.org/ns/1.0}p"
                        },
                        "action": {
                            "tag": "{http://www.tei-c.org/ns/1.0}interp",
                            "attrs": {
                                "{http://www.w3.org/XML/1998/namespace}id": "seq('note')"
                            }
                        }
                    },
                    {
                        "match": {
                            "tag": "{http://www.tei-c.org/ns/1.0}span",
                            "attrs": [
                                {
                                    "name": "style",
                                    "value": "font-weight-bold"
                                }
                            ]
                        },
                        "action": {
                            "tag": "{http://www.tei-c.org/ns/1.0}citedRange",
                            "attrs": {
                                "style": null,
                                "type": "page-line-ref"
                            }
                        }
                    }
                ]
            }
        },
        {
            "action": "extract-word-range"
        },
        {
            "action": "simplify-tree"
        },
        {
            "action": "strip-whitespace"
        }
    ],
    "post-process": [
        {
            "action": "regex-replace",
            "params": {
                "regex": "> ",
                "replace": ">"
            }
        }
    ]
}
