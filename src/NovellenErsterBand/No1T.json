{
    "workflow": [
        {
            "action": "trim-hidden"
        },
        {
            "action": "trim-empty"
        },
        {
            "action": "merge-styles",
            "params": {
                "extract_style_names": [
                    "margin-left",
                    "margin-right",
                    "margin-top",
                    "margin-bottom",
                    "text-align",
                    "text-indent",
                    "font-name",
                    "font-size",
                    "letter-spacing",
                    "font-style",
                    "font-weight",
                    "text-position"],
                "style_mappings": {
                    "[('font-name', 'Times New Roman'), ('font-size', '11pt'), ('margin-left', '0cm'), ('margin-right', '0cm'), ('text-align', 'center'), ('text-indent', '0cm')]": "paragraph text-center",
                    "[('font-name', 'Times New Roman'), ('font-size', '11pt'), ('margin-left', '0cm'), ('margin-right', '0cm'), ('text-align', 'justify'), ('text-indent', '0.501cm')]": "paragraph",
                    "[('font-name', 'Times New Roman'), ('font-size', '11pt'), ('margin-bottom', '0.212cm'), ('margin-left', '0cm'), ('margin-right', '0cm'), ('margin-top', '0.212cm'), ('text-align', 'justify'), ('text-indent', '0.501cm')]": "paragraph",
                    "[('font-name', 'Times New Roman'), ('font-size', '11pt'), ('margin-left', '0cm'), ('margin-right', '0cm'), ('text-align', 'justify'), ('text-indent', '1.27cm')]": "paragraph indent-big",
                    "[('font-name', 'Times New Roman'), ('font-size', '11pt'), ('margin-bottom', '0.282cm'), ('margin-left', '0cm'), ('margin-right', '0cm'), ('margin-top', '0cm'), ('text-align', 'justify'), ('text-indent', '1.27cm')]": "paragraph indent-big",
                    "[('font-name', 'Times New Roman'), ('font-size', '11pt'), ('margin-bottom', '0cm'), ('margin-left', '0cm'), ('margin-right', '0cm'), ('margin-top', '0.282cm'), ('text-align', 'justify'), ('text-indent', '0.501cm')]": "paragraph",
                    "[('font-name', 'Times New Roman'), ('font-size', '11pt'), ('margin-left', '0cm'), ('margin-right', '0cm'), ('text-align', 'justify'), ('text-indent', '0cm')]": "paragraph no-indent",
                    "[('font-name', 'Times New Roman'), ('font-size', '11pt'), ('margin-left', '0cm'), ('margin-right', '-0.049cm'), ('text-align', 'center'), ('text-indent', '0.501cm')]": "paragraph text-center",
                    "[('font-name', 'Times New Roman'), ('font-size', '11pt'), ('margin-left', '0cm'), ('margin-right', '-0.049cm'), ('text-align', 'center'), ('text-indent', '0cm')]": "paragraph no-indent text-center",
                    "[('font-name', 'Times New Roman'), ('font-size', '15pt'), ('margin-bottom', '0.741cm'), ('margin-left', '0cm'), ('margin-right', '0cm'), ('margin-top', '0cm'), ('text-align', 'center'), ('text-indent', '0cm')]": "heading-1",
                    "[('font-name', 'Times New Roman'), ('font-size', '15pt'), ('margin-bottom', '0.106cm'), ('margin-left', '0cm'), ('margin-right', '0cm'), ('margin-top', '0cm'), ('text-align', 'center'), ('text-indent', '0cm')]": "heading-1",
                    "[('font-name', 'Times New Roman'), ('font-size', '13pt'), ('margin-bottom', '0.529cm'), ('margin-left', '0cm'), ('margin-right', '0cm'), ('margin-top', '0cm'), ('text-align', 'center'), ('text-indent', '0cm')]": "heading-2",
                    "[('font-name', 'Times New Roman'), ('font-size', '13pt'), ('margin-bottom', '0.529cm'), ('margin-left', '0cm'), ('margin-right', '0cm'), ('margin-top', '0.106cm'), ('text-align', 'center'), ('text-indent', '0cm')]": "heading-2",
                    "[('font-name', 'Times New Roman'), ('font-size', '10pt'), ('margin-left', '0.501cm'), ('margin-right', '0cm'), ('text-align', 'justify'), ('text-indent', '-0.501cm')]": "paragraph no-indent font-size-small",
                    "[('font-name', 'Times New Roman'), ('font-size', '10pt'), ('margin-left', '0cm'), ('margin-right', '0cm'), ('text-align', 'justify'), ('text-indent', '0cm')]": "paragraph no-indent font-size-small",
                    "[('font-size', '15pt')]": "font-size-large",
                    "[('letter-spacing', '-0.004cm')]": "",
                    "[('letter-spacing', '-0.005cm')]": "",
                    "[('letter-spacing', '-0.007cm')]": "",
                    "[('letter-spacing', '-0.011cm')]": "",
                    "[('font-style', 'italic')]": "font-style-italic",
                    "[('font-style', 'italic'), ('letter-spacing', '-0.004cm')]": "font-style-italic",
                    "[('letter-spacing', '0.007cm')]": "",
                    "[('letter-spacing', '0.004cm')]": "",
                    "[('letter-spacing', '0.005cm')]": "",
                    "[('letter-spacing', '0.011cm')]": "",
                    "[('letter-spacing', '0.014cm')]": "",
                    "[('letter-spacing', '0.035cm')]": "",
                    "[('letter-spacing', '0.053cm')]": "letter-sparse",
                    "[('letter-spacing', '0.064cm')]": "letter-sparse",
                    "[('letter-spacing', '0.028cm')]": "letter-sparse",
                    "[('font-style', 'italic'), ('letter-spacing', '-0.007cm')]": "font-style-italic",
                    "[('font-style', 'italic'), ('letter-spacing', '0.011cm')]": "font-style-italic",
                    "[('font-style', 'italic'), ('letter-spacing', '0.007cm')]": "font-style-italic",
                    "[('font-name', 'Arial'), ('font-size', '10pt')]": "font-arial",
                    "[('font-style', 'italic'), ('letter-spacing', '-0.011cm')]": "font-style-italic",
                    "[('letter-spacing', '0.002cm')]": "",
                    "[('font-style', 'italic'), ('letter-spacing', '0.004cm')]": "font-style-italic",
                    "[('font-weight', 'bold')]": "font-weight-bold",
                    "[('font-size', '15pt'), ('letter-spacing', '0.053cm')]": "font-size-large letter-sparse",
                    "[('font-size', '13pt')]": "font-size-medium",
                    "[('font-size', '13pt'), ('letter-spacing', '0.053cm')]": "font-size-medium letter-sparse",
                    "[('text-position', 'super 58%')]": "sup",
                    "[('font-size', '11pt'), ('text-position', 'super 58%')]": "sup",
                    "[('font-name', 'Arial'), ('font-size', '10pt'), ('letter-spacing', '0.004cm')]": "font-arial",
                    "[('font-name', 'Arial'), ('font-size', '10pt'), ('letter-spacing', '-0.004cm')]": "font-arial"
                }
            }
        },
        {
            "action": "modify-elements",
            "params": {
                "rules": [
                    {
                        "match": {
                            "tag": "{http://www.tei-c.org/ns/1.0}p",
                            "attrs": [
                                {
                                    "name": "style",
                                    "value": "paragraph text-center font-size-large"
                                }
                            ]
                        },
                        "action": {
                            "tag": "{http://www.tei-c.org/ns/1.0}head",
                            "attrs": {
                                "style": null,
                                "type": "level-1"
                            }
                        }
                    },
                    {
                        "match": {
                            "tag": "{http://www.tei-c.org/ns/1.0}p",
                            "attrs": [
                                {
                                    "name": "style",
                                    "value": "heading-1"
                                }
                            ]
                        },
                        "action": {
                            "tag": "{http://www.tei-c.org/ns/1.0}head",
                            "attrs": {
                                "style": null,
                                "type": "level-1"
                            }
                        }
                    },
                    {
                        "match": {
                            "tag": "{http://www.tei-c.org/ns/1.0}p",
                            "attrs": [
                                {
                                    "name": "style",
                                    "value": "paragraph text-center font-size-medium"
                                }
                            ]
                        },
                        "action": {
                            "tag": "{http://www.tei-c.org/ns/1.0}head",
                            "attrs": {
                                "style": null,
                                "type": "level-2"
                            }
                        }
                    },
                    {
                        "match": {
                            "tag": "{http://www.tei-c.org/ns/1.0}p",
                            "attrs": [
                                {
                                    "name": "style",
                                    "value": "heading-2"
                                }
                            ]
                        },
                        "action": {
                            "tag": "{http://www.tei-c.org/ns/1.0}head",
                            "attrs": {
                                "style": null,
                                "type": "level-2"
                            }
                        }
                    },
                    {
                        "match": {
                            "attrs": [
                                {
                                    "name": "style",
                                    "value": "font-arial"
                                }
                            ]
                        },
                        "action": {
                            "tag": "{http://www.tei-c.org/ns/1.0}foreign",
                            "attrs": {
                                "style": null
                            }
                        }
                    },
                    {
                        "match": {
                            "tag": "{http://www.tei-c.org/ns/1.0}span",
                            "attrs": [
                                {
                                    "name": "style",
                                    "value": "font-style-italic"
                                }
                            ]
                        },
                        "action": {
                            "tag": "{http://www.tei-c.org/ns/1.0}pb",
                            "attrs": {
                                "style": null,
                                "n": "text()"
                            },
                            "text": null
                        }
                    },
                    {
                        "match": {
                            "tag": "{http://www.tei-c.org/ns/1.0}span",
                            "attrs": [
                                {
                                    "name": "style"
                                }
                            ]
                        },
                        "action": {
                            "tag": "{http://www.tei-c.org/ns/1.0}hi"
                        }
                    },
                    {
                        "match": {
                            "tag": "{http://www.tei-c.org/ns/1.0}span"
                        },
                        "action": {
                            "tag": "{http://www.tei-c.org/ns/1.0}seg"
                        }
                    }
                ]
            }
        },
        {
            "action": "simplify-tree"
        }
    ],
    "post-process": [
        {
            "action": "regex-replace",
            "params": {
                "regex": "n=\"\\[([0-9XVIxvi]+)\\]\\s*\"",
                "replace": "n=\"\\g<1>\""
            }
        },
        {
            "action": "regex-replace",
            "params": {
                "regex": "([a-z])<tei:span",
                "replace": "\\g<1> <tei:span"
            }
        },
        {
            "action": "regex-replace",
            "params": {
                "regex": "</tei:span>([a-z])",
                "replace": "</tei:span> \\g<1>"
            }
        }
    ]
}
